{
    "name": "rahmatawaludin/laravel-notes",
    "description": "Laravel 4 Artisan Notes Management",
    "license": "MIT",
    "authors": [
        {
            "name": "Rahmat Awaludin",
            "email": "rahmat.awaludin@gmail.com"
        }
