        "mockery/mockery": "0.8.0",
        "squizlabs/php_codesniffer": "1.*",
        "jakub-onderka/php-parallel-lint": "*",
        "jakub-onderka/php-console-highlighter": "dev-master",
        "phpmd/phpmd": "1.5.*"
    },
    "autoload": {
